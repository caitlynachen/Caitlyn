#include <iostream>
#include <string>
#include <algorithm>
#include <vector>

using namespace std;

int main() {
	
	//freopen ("evenodd.in", "r", stdin);
   // freopen ("evenodd.out", "w", stdout);
	
	int n;
	string num[100];
	char x;
	
	cin >> n;
	
	
	for(int i=0; i<n; i++){
		cin>> num[i];
	}
	
	for(int i=0; i<n; i++){
	

		x = num[i] [num[i].length()-1];
		
			if(((x-'0')%2)==0){
				cout << "even\n" ;
			}
			
			else{
				cout << "odd\n";
			}
			
			
		}
		
	}
	
	
	

	
	
	
	
